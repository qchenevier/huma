{
  "name": "huma",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "export": "nuxt-ts export",
    "serve": "nuxt-ts serve",
    "generate": "npm run build && npm run export",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt-ts build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt-ts build && DEPLOY_ENV=GH_PAGES nuxt-ts export",
    "serve:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt-ts serve",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@nuxt/typescript-runtime": "^0.4.10",
    "@nuxtjs/sitemap": "^2.4.0",
    "babel-runtime": "^6.26.0",
    "moment": "^2.27.0",
    "nuxt": "^3.0.0",
    "nuxt-buefy": "^0.3.31",
    "reading-time": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "push-dir": "^0.4.1"
  }
}
